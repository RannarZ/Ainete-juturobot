inar": 0, "colloquium": 0, "web_learning": 0, "independent_work": 146, "individual_work": 0, "internship": 0}, "prerequisites": [{"required": false, "uuid": "520b03be-9209-8375-be94-9897af02d8a3", "code": "FLKU.04.074", "title": {"en": "Folklore Studies", "et": "Folkloristika alused"}, "credits": 3, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36"}], "is_enlight_course": false}, "overview": {"study_languages": [{"code": "et", "et": "eesti keel", "en": "Estonian"}], "other_languages": [{"code": "en", "et": "inglise keel", "en": "English"}], "description": {"en": "Folklore has an active 'second life' in a number of artistic spheres - literature, film, figurative and decorative art, to name just a few. And Folkloristics (the study of folklore) necessarily touches upon many other disciplines (philology, sociology, religious studies, social and cultural history, etc.). This course aims to study the other forms folklore has been in dialogue with.", "et": "Rahvaluulel on aktiivne 'teine elu' paljudes kunstivaldkondades - kirjanduses, filmis, kujutavas kunstis ja tarbekunstis, kui nimetada vaid mõnda. Ja folkloristika (folkloristika uurimine) puudutab paratamatult paljusid teisi distsipliine (filoloogiat, sotsioloogiat, religiooniteadust, sotsiaal- ja kultuurilugu jne). Seega on selle kursuse eesmärk uurida teisi vorme, millega folkloor on dialoogis olnud."}, "objectives": [{"en": "To see how artists have used folklore material, and to understand what folkloristics can learn from other disciplines and schools of thoughts, and what other disciplines can learn from folkloristics.", "et": "Näha, kuidas kunstnikud on folkloorimaterjali kasutanud, ning mõista, mida folkloristika võib teistelt distsipliinidelt ja mõttekoolkondadelt õppida ning mida teised distsipliinid folkloristikast."}], "learning_outcomes": [{"en": "The learning outcomes for this course are:", "et": "Selle kursuse õpiväljundid on järgmised:"}, {"en": "1) to gain an understanding of the disciplines and schools of thought relevant in the study of folklore", "et": "1) omandada arusaam rahvaluule uurimisel olulistest distsipliinidest ja koolkondadest "}, {"en": "2) to be able to critically assess these disciplines and schools of thought", "et": "2) osata neid distsipliine ja mõttekoolkondi kriitiliselt hinnata"}], "notes": {}}, "grading": {"independent_work": {"et": "Kursuse jooksul kirjutatakse essee ja vajalik on moodle'sse ülespandud lugemismaterjali läbi töötamine.\nVähemalt 60 tunni alatuses loetakse materjale."}, "grade_preconditions": {"et": "Osalenud vähemalt 75% ulatuses loengutes."}, "grade_evaluation": {"et": "Lõpphindamisel moodustab 60% hindest kursusetöö ja 40% essee."}, "debt_elimination": {"et": "Ei ole."}, "assessment_scale": {"code": "grade", "et": "Eristav (A, B, C, D, E, F, mi)", "en": "differentiated (A, B, C, D, E, F, not present)"}, "results_available_from": {}}, "resources": {"web_based": {"code": "2", "et": "põimõpe", "en": "blended learning"}, "website_url": "https://moodle.ut.ee", "is_moodle_connected": true, "learning_environments": [{"uuid": "ca04a89e-4a06-4950-bfb9-b9346fd3c184", "type": "MOODLE", "code": "HVKU.04.047", "base_url": "undefined", "is_auto_update": true, "is_default": true, "is_auto_grading": false}]}, "participants": {"lecturers": [{"person_uuid": "9a3d9138-5bf9-5d90-b3da-20fc74e4e5bb", "person_name": "Jonathan Edward Hodgess Roper", "structural_unit": {"code": "HVKU04", "name": {"en": "Department of Estonian and Comparative Folklore", "et": "eesti ja võrdleva rahvaluule osakond"}, "phone": "+372 737 5304", "address": "EST", "level": 3, "academic": true, "supports_continuous_learning": false, "parent_code": "HVKU"}, "is_responsible": true, "is_teaching": true}, {"person_uuid": "df2d8b3f-09b1-50ea-c652-8aa960296ac3", "person_name": "Reeli Reinaus", "structural_unit": {"code": "HVKU", "name": {"en": "Institute of Cultural Research", "et": "kultuuriteaduste instituut"}, "emai