els": [{"code": "bachelor", "et": "bakalaureuseõpe", "en": "bachelor's studies"}], "hours": {"lecture": 0, "practice": 0, "seminar": 30, "colloquium": 0, "web_learning": 0, "independent_work": 48, "individual_work": 0, "internship": 0}, "is_enlight_course": false}, "overview": {"study_languages": [{"code": "et", "et": "eesti keel", "en": "Estonian"}], "description": {"en": "The aim of the seminar is to provide students with necessary skills from collecting research material and working with academic literature to writing and editing academic publications. The aim of the research seminar is to write an independent academic work in dialogue with academic supervisor and colleagues. In addition to written research, students must present presentations that are based on their research and scientific literature of the field. In the end of academic year the public defence of the seminar thesis is carried thought.", "et": "Eriseminari eesmärgiks on anda üliõpilastele teaduslikuks tööks vajalikud teadmised ja oskused uurimismaterjali kogumisest ja teaduskirjandusega töötamisest kuni valmis töö vormistamiseni. Eriseminari eesmärgiks on iseseisva teadusliku uurimistöö kirjutamine dialoogis juhendaja ja kolleegidega. Lisaks kirjalikule uurimistööle tuleb teha suulisi ettekandeid oma teemat puudutava teaduskirjanduse  ja oma töö erinevate osade põhjal. Õppeaasta lõpul toimub valminud seminaritöö avalik kaitsmine."}, "objectives": [{"en": "Basic overview is given on composing academic work in regular communication with academic supervisor and students of the similar topics.", "et": "Antakse esmane ülevaade teadustöö koostamisest regulaarses suhtlemises juhendajaga ning lähedase problemaatikaga tegelevate üliõpilastega."}], "learning_outcomes": [{"en": "Basic knowledge for conducting academic research are gained - formulating the research question, working with literature, selecting and using research methods, writing academic text.", "et": "Omandatakse esmased oskused ja kogemused teadustöö tegemiseks - probleemi püstitamine, töö teaduskirjandusega, metoodika valik ja rakendamine, akadeemilise töö kirjutamine."}], "notes": {"et": "Õppetöö algab 3. õppenädalal, kõigi seminaride töö sisu tutvustava üldkoosolekuga."}}, "grading": {"independent_work": {"et": "Isesisvate tööde loetelu koostatakse iga seminaritöö puhul individuaalselt koostöös juhendajaga."}, "independent_work_assessments": [{"work_type": {"code": "project", "et": "Projekt", "en": "project"}, "description": {"et": "Esimese osa lõpuks on kokku lepitud juhendaja, koostöös temaga määratletud tulevase uurimisitöö teema ja üldine plaan.\nTeise osa lõpuks valmib juhendajaga koostöös seminaritöö."}}], "grade_preconditions": {"et": "Seminarides osalemine"}, "grade_evaluation": {"et": "Lõplik tulemus saadakse pärast seminaritöö avalikku kaitsmist kevadsemestri lõpus."}, "debt_elimination": {"et": "vastavalt semiootika osakonna akadeemilisele kalendrile"}, "assessment_scale": {"code": "pass", "et": "Eristamata (arv, m.arv, mi)", "en": "non-differentiated (pass, fail, not present)"}, "results_available_from": {}}, "resources": {"web_based": {"code": "2", "et": "põimõpe", "en": "blended learning"}, "website_url": "http://moodle.ut.ee", "is_moodle_connected": true, "learning_environments": [{"uuid": "a348392c-bc3d-4bf3-829b-86fc0c05b9b2", "type": "MOODLE", "code": "FLSE.00.150", "base_url": "undefined", "linked_to_groups": ["6d553027-de73-5c18-743a-e9cfb7c99b71", "e40a9b70-165b-637e-5099-6f51ead39290", "29946cb4-529c-7cbe-637d-62fc688de544"], "is_auto_update": false, "is_default": true, "is_auto_grading": false}]}, "participants": {"lecturers": [{"person_uuid": "c74fc9fc-6ecc-d711-1dc5-c5aa5680bedf", "person_name": "Kalevi Kull", "structural_unit": {"code": "HVFI04", "name": {"en": "Department of Semiotics", "et": "semiootika osakond"}, "phone": "+372 737 5933", "webpage_url": "https://flfi.ut.ee/et/semiootika-osakond", "address": "Jakobi 2, III korrus, ruumid 302-314, 317-318\n51005 Tartu lin