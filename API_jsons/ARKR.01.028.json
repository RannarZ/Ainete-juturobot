: 2, "academic": true, "supports_continuous_learning": true, "parent_code": "MV"}, "is_responsible": false, "is_teaching": true, "study_group_roles": {"2. rühm": {"code": "2", "et": "Praktikumijuhendaja", "en": "supervisor of practical class"}, "3. rühm": {"code": "2", "et": "Praktikumijuhendaja", "en": "supervisor of practical class"}, "5. rühm": {"code": "2", "et": "Praktikumijuhendaja", "en": "supervisor of practical class"}, "9. rühm": {"code": "2", "et": "Praktikumijuhendaja", "en": "supervisor of practical class"}, "7. rühm": {"code": "2", "et": "Praktikumijuhendaja", "en": "supervisor of practical class"}, "15. rühm": {"code": "2", "et": "Praktikumijuhendaja", "en": "supervisor of practical class"}, "8. rühm": {"code": "2", "et": "Praktikumijuhendaja", "en": "supervisor of practical class"}}}], "study_groups": [{"uuid": "e466d6f2-bdf5-7177-4298-ec8521cf3d0e", "code": "2. rühm"}, {"uuid": "f1efbc33-b89f-ae52-2458-491bbefc4d8a", "code": "3. rühm"}, {"uuid": "7db15ef8-5bd6-0e90-73c0-43be4b7f4ab2", "code": "5. rühm"}, {"uuid": "23c5b4fa-44ca-9408-0b3d-ae57b3b7b716", "code": "8. rühm"}, {"uuid": "f8e2c066-ddba-79e7-8f35-d463be09864d", "code": "9. rühm"}, {"uuid": "096bf59e-38c3-5212-f1b9-85408a838272", "code": "7. rühm"}, {"uuid": "889923c2-c1b0-b407-fb43-f39646acf3be", "code": "15. rühm"}]}, "schedule": {"weeks": {}, "entries": [{"time": "2018-09-24", "work_type": {"code": "lecture", "et": "loeng", "en": "lecture"}, "topic": {}, "description": {"et": "Ninaneelumandli hüpertroofia ja selle tüsistused. Äge ja krooniline tonsilliit. Peritonsillaarne abstsess. I. Aus\nKõri arenguhäired. Äge ja krooniline larüngiit. Äge epiglotiit. Subglotiline larüngiit. Häälehäired. Häälepaelte pareesid. Kõri kasvajad. R. Ani\nUneapnoe. M. Veldi"}}, {"time": "2018-09-25", "work_type": {"code": "lecture", "et": "loeng", "en": "lecture"}, "topic": {}, "description": {"et": "Näo ja suupõhja, neelu ja kaela pehmete kudede põletikud, kasvajad, tsüstid ja fistlid. Väliskõrva haigused. Krooniline keskkõrvapõletik. Audioloogia ja vestibuloogia. P. Kasenõmm"}}, {"work_type": {"code": "practice", "et": "praktikum", "en": "practical session"}, "topic": {}, "description": {"et": "Propedutika I - Kuperjanovi 1.\nSissejuhatus erialasse. Kõrva-nina-kurguarsti töökoht ja töövahendid. Kõri, trahhea ja bronhide kliiniline anatoomia, füsioloogia ja patoloogia. Suu, neelu ja söögitoru kliiniline anatoomia, füsioloogia ja patoloogia.\nUurimis- ja vaatlusõtted ning nende harjutamine.\ndr I.Aus, dr P.Kasenõmm, dr R.Ani, dr N.Lieberg, dr M.Suurna, dr V.Ristioja, dr M.Veldi"}}, {"work_type": {"code": "practice", "et": "praktikum", "en": "practical session"}, "topic": {}, "description": {"et": "Propedutika II - Kuperjanovi 1.\nNina ja ninakrõvalkoobaste kliiniline anatoomia, füsioloogia ja patoloogia. Tonsillid ja adenoidid.\nUurimis- ja vaatlusõtted ning nende harjutamine.\ndr I.Aus, dr P.Kasenõmm, dr R.Ani, dr N.Lieberg, dr M.Suurna, dr V.Ristioja, dr A.Põld, dr Marlit Veldi"}}, {"work_type": {"code": "practice", "et": "praktikum", "en": "practical session"}, "topic": {}, "description": {"et": "Propedutika III - Kuperjanovi 1.\nKõrva kliiniline anatoomia, füsioloogia ja patoloogia.\nUurimis- ja vaatlusõtted ning nende harjutamine.\ndr I.Aus, dr P.Kasenõmm, dr R.Ani, dr N.Lieberg, dr M.Suurna, dr V.Ristioja, dr A.Põld, dr M.Veldi"}}, {"work_type": {"code": "practice", "et": "praktikum", "en": "practical session"}, "topic": {}, "description": {"et": "Kuulmis- ja kõnestamiskeskus I - Kuperjanovi 1.\nKuulmisuuringud - impedantsmeetria (tümpanomeetria, staapesrefleksid); toonaudiomeetrilised uuringud (läveline, üleläveline ja vabavälja meetod); kõneaudiometria; kuuldeaparaadid - kõrvatagused,-sisesed ja tasakaaluaparaadid. Kuulmise rehabilitatsioon. Lineaarsed, mittelineaarse, ülekanalilised, mitmekanalilised ja täisdigitaalsed kuuldeaparaadid. FM süsteemid. Kuuldeaparaatide valik, testimine, sobitamine. Individuaalsed kõrvaotiskud. Muud tehnilised abivahendid kuulmislanguse korral - alarmsüsteemid, telefonivõimendid jne. Kõrvakohinate diagnostika, ravi (medikamentoosne, statsionaarne ja ambulatoorne) ja rehabilitatsioon (sobivate harjutuskomplekside õpetamine, tinnituse ümberõpe). Äkk-kurtus (põhjused, diagnostika ja ravi).\ndr K.Kruustük, dr R.Tikk"}}, {"work_type": {"code": "practice", "et": "praktikum", "en": "practical session"}, "topic": {}, "description": {"et": "Kuulmis- ja kõnestamiskeskus II - Kuperjanovi 1.\nVestibulaaraparaadi uurimine - ENG ja VNG. Nüstagmid. Pearingluse ja tasakaaluhäirete põhjused, diagnostika, diferebtsiaaldiagnostika ja ravi. Kuulmislangus lapseeas (põhjused, diagnoosimine ja rehabilitatsioon). OAE ja BEAR uuringud. Susejõrva implantatsioon. Logopeediline rehabilitasioon - kõne-ja häälehäiretega patsientide ravi ja rehabilitatsioon\ndr K.Kruustük, dr R.Tikk"}}]}, "registration_info": {"audience": {}, "min_students": 5}, "parent_uuid": "24f6801e-3e05-4b34-638a-65b4e90d19d8", "parent_code": "ARKR.01.028", "parent_credits": 4, "_actions": {"is_moodle_editable": false, "is_editable": false, "is_deletable": false}}