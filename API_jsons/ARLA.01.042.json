practice": 27, "seminar": 45, "colloquium": 0, "web_learning": 0, "independent_work": 89, "individual_work": 0, "internship": 0}, "prerequisites": [{"required": true, "uuid": "21eb468a-9a12-43e0-94c7-78a7cf4ff623", "code": "AR00.00.066", "title": {"en": "Surgery", "et": "Kirurgia"}, "credits": 12, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-04-01T15:21:36"}, {"required": true, "uuid": "e979d518-d3bb-d364-882f-b96ebf9d7c40", "code": "MV00.00.015", "title": {"en": "Internal Medicine", "et": "Sisehaigused"}, "credits": 28, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-04-01T15:19:05", "alternatives": [{"uuid": "5c387d45-b118-4ab8-c88a-b4f5cbf65b8b", "code": "AR00.00.065", "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36", "title": {"en": "Internal Medicine", "et": "Sisehaigused"}, "credits": 27, "type": {"code": "complex", "et": "Kompleksaine", "en": "Complex course"}}]}], "is_enlight_course": false}, "overview": {"study_languages": [{"code": "et", "et": "eesti keel", "en": "Estonian"}], "description": {"en": "The course introduces the growth and development of the child from birth up to the end of adolescent age, teaches to recognize and be aware of diagnosis, prevency and treatment of common diseases  in infancy and childhood.", "et": "Käsitletakse lapse kasvamist ja arenemist sünnist puberteedini, organsüsteemide ealisi iseärasusi, imiku hooldust, toitmist, haiguste ennetamist, diagnoosimist ja ravi."}, "objectives": [{"en": "The aim of course is to teach basic knowledges in breast-feeding policy, in child development, developmental disorders, principles of treatment and management of acute and chronic diseases/ disorders, to obtain clinical skills in observation of the child, to learn the possible reasons of disturbances of childhood growth, to recognize common diseases in infancy and childhood  (anaemia, infectious diseases etc).", "et": "Aine õpetamise eesmärgiks on tutvustada terve lapse arengut, tervisliku kasvatuse printsiipe, lastehaiguste sümptomaatikat ja diagnoosimist, diferentsiaaldiagnoosimist, haiguste raviprintsiipe, preventsiooni  ja intensiivravi põhimõtteid."}], "learning_outcomes": [{"en": "After completing the course, the student will:", "et": "Kursuse läbinud üliõpilane "}, {"en": "*have knowlegde about  the child's developmental  phases and their age-specific characteristics, ", "et": "*teab lapse arenguperioode ja nende iseärasusi, "}, {"en": "*know the basic principles of growing process and prevention of diseases, ", "et": "*tunneb kasvamise ja haiguste ennetamise põhitõdesid, "}, {"en": "*have theoretical overview of  the peculiarities of the  different organ-systems in childhood, ", "et": "*omab ülevaadet  organsüsteemide iseärasustest lapseeas,"}, {"en": "*know the basics of feeding and care of infant and children  ", "et": "*teab imiku ning väikelapse toitmise ja hoolduse algtõdesid,"}, {"en": "*have overview of the common diseases in pediatric population, including diagnostic principles and treatment strategies, ", "et": "*omab ülevaadet sagedamini esinevatest lastehaigustest, sealhulgas diagnostika ja ravi põhimõtetest, "}, {"en": "*be able to communicate with children of different ages and their parents, ", "et": "*on võimeline suhtlema erinevas vanuses laste ja nende vanematega, "}, {"en": "*be able to take an history himself, to perform a basic examination of  child and to estimate the most important signs and symptoms of the diseases.", "et": "*suudab iseseisvalt võtta anamneesi, teostada lapse objektiivset läbivaatust ja hinnata olulisemaid haigustunnuseid"}], "notes": {}}, "grading": {"independent_work": {}, "independent_work_assessments": [{"work_type": {"code": "written", "et": "Kirjalik lõpphindamine", "en": "final written assessment"}, "description": {"et": "Eksami hindamine: test 50-46 punkti (A), 45-41 punkti (B), 40-36 punkti (C), 35-3