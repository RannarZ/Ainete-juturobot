ours": {"lecture": 0, "practice": 15, "seminar": 0, "colloquium": 0, "web_learning": 0, "independent_work": 30.5, "individual_work": 0, "internship": 0}, "prerequisites": [{"required": true, "uuid": "3a8d6968-0cee-caef-f54f-b1b23145433b", "code": "MV00.00.039", "title": {"en": "Fundamentals of Diagnostics", "et": "Diagnostika alused"}, "credits": 16, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-04-01T15:12:20", "alternatives": [{"uuid": "62901d09-3d45-ffd9-06b8-8df341b867b7", "code": "AR00.00.016", "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36", "title": {"en": "Fundamentals of Diagnostics", "et": "Diagnostika alused"}, "credits": 17, "type": {"code": "complex", "et": "Kompleksaine", "en": "Complex course"}}]}], "is_enlight_course": false}, "overview": {"study_languages": [{"code": "et", "et": "eesti keel", "en": "Estonian"}], "description": {"en": "Diagnostic and treatment principles, methods and tactics of urological diseases. The course is a part of the subject \"Surgery\" (12 CP)", "et": "Uroloogiliste haiguste diagnoosimise ja ravi printsiibid, metoodika ja taktika. Antud aine moodustab ühe osa õppeainest \"Kirurgia\" (12 AP)"}, "objectives": [{"en": "To teach the epidemiology, etio-pathogenesis, urodiagnostics, classification of diseases, clinical picture, treatment principles (open surgery and endoscopic surgery), prognosis and prophylaxis of urological and uro-oncological diseases, including more common uroogical pathologies.", "et": "Õpetada uroloogiliste ja uroonkoloogiliste haiguste epidemioloogiat, etiopatogeneesi, haiguste klassifikatsiooni, kliinilist pilti, raviprintsiipe (avatud ja laparoskoopiline kirurgia), prognoose ja profülaktikat."}], "learning_outcomes": [{"en": "After completing the course, the student", "et": "Kursuse läbinud üliõpilane:"}, {"en": "1) is able to diagnose more common urological pathologies;", "et": "1) oskab diagnoosida sagedasemaid uroloogilise patoloogiaid;"}, {"en": "2) has an understanding of the performing of diagnostic procedures ad their indications;", "et": "2) omab ettekujutust diagnostiliste protseduuride läbiviimisest, näidustustest;"}, {"en": "3) has general comprehension of contemporary treatment principles of urological diseases.", "et": "3) omab ülevaadet uroloogiliste haiguste tänapäevastest raviprintsiipidest."}], "notes": {"et": "Iseseisva õppe plaan\n1.       Edastatakse lisamaterjale iseõppimiseks: ülevaateartiklid üldarsti tasemel või muud lühikokkuvõtted teemast\n2.       Moodlesse lisame praktikumi teemade kavad et oleks lihtsam struktureeritud õppida\n3.       Vähemalt üks videoseminar rühma kohta, kui see töötab sujuvalt siis vajadusel ka rohkem\n4.       Edastatakse protseduuride nimekiri mida enamasti on tudengitele näidatud tsüklis, mida igaüks peab üle vaatama videoõppematerjalist\n5.       Enesekontrolli test Moodlesse\n6.       Kõigile saadetakse lahendamiseks/konsulteerimiseks üks lühike kliiniline juht\n7.       Pakume ühte kokkuvõtvat seminari olulisemate teemade kordamiseks, kontrollküsimusteks jne rühma kohta (juhul kui enne eksamit on vaba aega võimalik leida)"}}, "grading": {"independent_work": {}, "independent_work_assessments": [{"work_type": {"code": "written", "et": "Kirjalik lõpphindamine", "en": "final written assessment"}, "description": {"et": "Kirjalik eksam, osa koondeksamist Kirurgia"}}], "grade_preconditions": {"et": "Praktikumides osalemine"}, "grade_evaluation": {"et": "1 osa koondeksamist \"Kirurgia\""}, "debt_elimination": {"et": "Üksikute puudumiste korral referaat, suurema puudumise korral tuleb tsükkel järgi teha. Õppejõud otsustab sõltuvalt epidemioloogilisest olukorrast."}, "assessment_scale": {"code": "pass", "et": "Eristamata (arv, m.arv, mi)", "en": "non-differentiated (pass, fail, not present)"}, "results_available_from": {}}, "resources": {"web_based": {"code": "2", "et": "põimõpe", "en": "blended learning"}, "website