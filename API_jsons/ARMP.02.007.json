{"uuid": "6f69029f-9b7f-e3be-3db3-d5a1f2afa292", "code": "mv-2018-autumn-fulltime-c5", "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2023-12-21T16:34:08", "title": {"en": "Immunology", "et": "Immunoloogia"}, "credits": 5, "credit_type": "ECTS", "target": {"year": {"code": "2018", "et": "2018/2019", "en": "2018/2019"}, "semester": {"code": "autumn", "et": "sügis", "en": "autumn"}, "study_type": {"code": "fulltime", "et": "päevaõpe", "en": "regular studies"}, "language": {"code": "et", "et": "eesti keel", "en": "Estonian"}, "part": {"code": "0", "et": "Osadeta", "en": "None"}, "faculty": {"code": "MV", "name": {"en": "Faculty of Medicine", "et": "meditsiiniteaduste valdkond"}, "address": "Tartu linn, \nTartu linn, Tartumaa \nEST", "city": "Tartu linn", "level": 1, "academic": true, "supports_continuous_learning": true}, "course_main_structural_unit": {"code": "MVBS06", "name": {"en": "Department of Immunology", "et": "immunoloogia osakond"}, "webpage_url": "https://biomeditsiin.ut.ee/et/sisu/immunoloogia-osakond", "address": "EST", "level": 3, "academic": true, "supports_continuous_learning": false, "parent_code": "MVBS"}, "label": {"en": "18/19 A regular MV", "et": "18/19 S PÕ MV"}}, "general": {"year": {"code": "2018", "et": "2018/2019", "en": "2018/2019"}, "input_languages": [{"code": "et", "et": "eesti keel", "en": "Estonian"}], "structural_unit_shares": [{"structural_unit_code": "MVBS06", "structural_unit_name": {"en": "Department of Immunology", "et": "immunoloogia osakond"}, "course_share_percent": 100, "coordinating": true}], "type": {"code": "regular", "et": "Tavaline aine", "en": "Regular course"}}, "additional_info": {"is_vota_course": true, "is_continuous_learning_course": false, "study_levels": [{"code": "bachelor_master", "et": "integreeritud bakalaureuse- ja magistriõpe", "en": "integrated bachelor's and master's studies"}], "hours": {"lecture": 22, "practice": 0, "seminar": 43, "colloquium": 0, "web_learning": 0, "independent_work": 65, "individual_work": 0, "internship": 0}, "prerequisites": [{"required": true, "uuid": "564ff9eb-0e62-874f-3932-0b0db25acae7", "code": "ARAN.01.038", "title": {"en": "Anatomy", "et": "Anatoomia"}, "credits": 12, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36"}, {"required": true, "uuid": "5d671478-f349-fdc0-aff5-de8f668e57c2", "code": "ARAN.02.005", "title": {"en": "Histology", "et": "Histoloogia"}, "credits": 8, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36"}, {"required": true, "uuid": "97876184-2711-8639-4161-568512a69a7f", "code": "ARMP.01.029", "title": {"en": "Biology", "et": "Bioloogia"}, "credits": 10, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36"}, {"required": true, "uuid": "fe2fc2f8-4397-cd3f-70a5-0647cdc79984", "code": "ARTH.02.056", "title": {"en": "History of Medicine", "et": "Meditsiiniajalugu"}, "credits": 3, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36"}], "is_enlight_course": false}, "overview": {"study_languages": [{"code": "et", "et": "eesti keel", "en": "Estonian"}, {"code": "en", "et": "inglise keel", "en": "English"}], "description": {"en": "Basic knowledges about cellular and molecular aspects of immune system are given. Also we explain possibilities for evaluation of immune system function and treatment in clinical settings.", "et": "Aine annab baasteadmised immunsüsteemi funktsioneerimise rakulistest ja molekulaarsetest alustest ning immuunsüsteemi seisundi hindamise ja mõjustamise põhilistest kliinilistest võimalustest."}, "objectives": [{"en": "To get basic knowledges for the further studies in clinical medicine.", "et": "Ettevalmistus kliiniliste distsipliinide omandamiseks arstiõppes"}], "learning_outcomes": [{"en": "At the end of the course, within the syllabus, a student: ", "et": "Õppeaine läbimise järgselt, vastavalt programmile, üliõpilane:"}, {"en": "1. is acquainted with the basic but up-to-date knowledge about the immune system;", "et": "1. on omandanud nüüdisaegsel tasemel baasteadmised immuunsüsteemi funktsioneerimisest;"}, {"en": "2. knows and understands the cellular and molecular mechanisms of the immune system;", "et": "2. tunneb ja mõistab immuunsüsteemi funktsioneerimise rakulisi ja molekulaarseid aluseid;"}, {"en": "3. knows the general properties of the diseases and pathological conditions which are immune system related, is able to understand the corresponding immune processes in the human organism, is capable to give examples of most common disorders in immune system;", "et": "3. teab üldjoontes immuunsüsteemi funktsiooni muutusega seotud sagedamini esinevaid haigusi ja seisundeid, suudab aru saada immuunsüsteemiga seotud protsessidest inimorganismis, oskab tuua olulisemate seisundite kohta näiteid;"}, {"en": "4. knows how to evaluate the status of the immune system and corresponding options for immune modulation, knows immunological methods and their use in diagnosis;", "et": "4. teab immuunsüsteemi seisundi hindamise ja mõjutamise võimalusi, immunoloogilisi meetodeid ja nende kasutamist diagnostikas;"}, {"en": "5. is able to navigate in the modern medical literature on the field of immunology, is able to find specialized scientific articles and to complement its own knowledge about the immunology", "et": "5. oskab orienteeruda kaasaegses meditsiinilise immunoloogia alases erialakirjanduses, oskab üles leida erialaartiklid ja oma teadmisi iseseisvalt täiendada"}], "notes": {}}, "grading": {"independent_work": {"et": "Vähemalt ühe ettekande tegemine (vastavalt kursuse algul koostatud plaanile)."}, "independent_work_assessments": [{"work_type": {"code": "test", "et": "Kontrolltöö", "en": "test"}, "description": {"et": "Eristav hindamine.\nKontrolltöö immunoloogia üldosast, esimeses 4 loengus ja kahes sissejuhatavas seminaris käsitletud materjali, samuti loengus näidatud iseseisvalt omandatava materjali kohta. Kontrolltöö koosneb 30 punktist (max 30 palli). \nHinne A - 28-30 p; B - 25-27 p; C - 22-24 p; D - 19-21 p; E - 16-18 p; F - 15 ja vähem palli. \nSelle kontrolltöö hinne (hinded) osaleb (osalevad) lõpliku eksamihinde kujunemisel 25% osakaaluga."}}, {"work_type": {"code": "test", "et": "Kontrolltöö", "en": "test"}, "description": {"et": "Mitteeristav hindamine.\nKontrolltöö kliinilisest immunoloogiast (allergia, autoimmuunsus, infektsioonid, immuundefitsiidid, transplantatsiooni ja kasvajate immunoloogia) sooritamiseks on vaja valikvastustega testis saada vähemalt 75% maksimumpunktidest. Selle kontrolltöö tulemus eksamihinde kujunemist ei mõjuta."}}, {"work_type": {"code": "written", "et": "Kirjalik lõpphindamine", "en": "final written assessment"}, "description": {"et": "Kirjalik eksam, koosneb 3-5 küsimusest essee-tüüpi vastustega. Eksamitöö kirjutamiseks on aega 120 minutit.\n\nA- Üliõpilane tunneb suurepäraselt immuunsüsteemi funktsiooni põhimõtteid, immuunsüsteemi funktsioneerimise rakulisi ja molekulaarseid aluseid, immuunsüsteemi funktsioonidega seotud haigusi ja patoloogilisi seisundeid, immuunsüsteemi seisundi hindamise ja mõjustamise võimalusi, immunoloogiliste meetodite põhimõtteid ja neil põhinevaid diagnostilisi teste etteantud programmi ulatuses.  Vastus näitab ja peegeldab üliõpilase sügavaid teadmisi, sisulist ja süstemaatilist arusaamist teemast, võimet loogiliselt seostada erinevaid teadmisi. Vastuse teksti ortograafia ja semantika on selge ja ühemõtteline. Vastus on fokusseeritud küsimusele ega esine teemast kõrvalekaldumist.  \nB- Üliõpilane tunneb väga hästi immuunsüsteemi funktsiooni põhimõtteid, immuunsüsteemi funktsioneerimise rakulisi ja molekulaarseid aluseid, immuunsüsteemi funktsioonidega seotud haigusi ja patoloogilisi seisundeid, immuunsüsteemi seisundi hindamise ja mõjustamise põhilisi võimalusi, immunoloogiliste meetodite põhimõtteid ja neil põhinevaid diagnostilisi teste etteantud programmi ulatuses.  Vastus näitab/peegeldab üliõpilase väga head arusaamist teemast. \nC- Üliõpilane tunneb hästi immuunsüsteemi funktsiooni põhimõtteid, immuunsüsteemi funktsioneerimise rakulisi ja molekulaarseid aluseid, immuunsüsteemi funktsioonidega seotud haigusi ja patoloogilisi seisundeid, immuunsüsteemi seisundi hindamise ja mõjustamise põhilisi võimalusi, immunoloogiliste meetodite põhimõtteid ja neil põhinevaid diagnostilisi teste etteantud programmi ulatuses. Vastus näitab ja peegeldab üliõpilase head arusaamist teema peamistest ja olulistest aspektidest.\nD- Üliõpilane tunneb olulisemaid immuunsüsteemi funktsiooni põhimõtteid, immuunsüsteemi funktsioneerimise rakulisi ja molekulaarseid aluseid, immuunsüsteemi funktsioonidega seotud haigusi ja patoloogilisi seisundeid, immuunsüsteemi seisundi hindamise ja mõjustamise põhilisi võimalusi, immunoloogiliste meetodite põhimõtteid ja neil põhinevaid diagnostilisi teste etteantud programmi ulatuses. Vastus näitab ja peegeldab üliõpilase arusaamist teema peamistest ja olulistest aspektidest, võib esineda üksikuid ja laadilt kergemaid eksimusi.\nE- Üliõpilase teadmised  immuunsüsteemi funktsiooni põhimõtetest, immuunsüsteemi funktsioneerimise rakulistest  ja molekulaarsetest alustest, immuunsüsteemi funktsioonidega seotud haigustest ja patoloogilistest seisunditest,  immuunsüsteemi seisundi hindamise ja mõjustamise põhilistest võimalustest, immunoloogiliste meetodite põhimõtetest ja diagnostikast on väga üldised ja osaliselt puudulikud, kuid mitte täiesti valed."}}], "grade_preconditions": {"et": "Kontrolltööd on sooritatud positiivselt, praktikumidest-seminaridest osavõtt, võlgnevused puuduvad."}, "grade_evaluation": {"et": "Lõplik hinne kujuneb kirjalikul eksamil vastavalt üliõpilase kirjalikult väljendatud teadmistele ja arusaamadele eksamiküsimuste kohta (75% osakaal) ja 1. kontrolltöö hinde (osakaal 25%) alusel. Eksamil tuleb vastata kolmele (kuni viiele) küsimusele essee-tüüpi vastustega"}, "debt_elimination": {"et": "Põhjusega puudutud seminaride ja praktikumide kohta tuleb kirjutada ja esitada nõuetele (10000 tähemärki koos tühikutega, allikaviidete loendit arvestamata) vastavad referaadid.\nKõik võlgnevused peavad olema likvideeritud eksamisessiooni alguseks."}, "assessment_scale": {"code": "grade", "et": "Eristav (A, B, C, D, E, F, mi)", "en": "differentiated (A, B, C, D, E, F, not present)"}, "results_available_from": {}}, "resources": {"web_based": {"code": "2", "et": "põimõpe", "en": "blended learning"}, "website_url": "http://moodle.ut.ee", "is_moodle_connected": false, "mandatory_materials": "Immunoloogia. Õpik kõrgkoolidele, R Uibo et al.\n\nBasic immunology: functions and disorders of the immune system \nAbul K. Abbas, Andrew H. Lichtman\nSaunders Elsevier", "recommended_materials": "Janeway's immunobiology \nKenneth Murphy\nGarland Science"}, "participants": {"lecturers": [{"person_uuid": "9c015327-682f-7b3c-2cd5-3f8c4818b556", "person_name": "Karin Kannel", "structural_unit": {"code": "MV", "name": {"en": "Faculty of Medicine", "et": "meditsiiniteaduste valdkond"}, "address": "Tartu linn, \nTartu linn, Tartumaa \nEST", "city": "Tartu linn", "level": 1, "academic": true, "supports_continuous_learning": true}, "is_responsible": false, "is_teaching": true}, {"person_uuid": "3157d491-ed8c-2870-aeaa-1dc205283091", "person_name": "Kai Kisand", "structural_unit": {"code": "MVBS04MP", "name": {"en": "Molecular Pathology Research Group", "et": "molekulaarpatoloogia uurimisgrupp"}, "webpage_url": "https://molekulaarpatoloogia.ut.ee", "address": "EST", "level": 4, "academic": false, "supports_continuous_learning": false, "parent_code": "MVBS04"}, "is_responsible": false, "is_teaching": true, "study_group_roles": {"2. rühm": {"code": "1", "et": "Lektor", "en": "lecturer"}, "3. rühm": {"code": "1", "et": "Lektor", "en": "lecturer"}, "5. rühm": {"code": "1", "et": "Lektor", "en": "lecturer"}, "9. rühm": {"code": "1", "et": "Le