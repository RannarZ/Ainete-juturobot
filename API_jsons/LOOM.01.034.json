individual_work": 0, "internship": 0}, "prerequisites": [{"required": true, "uuid": "9fd47766-49a7-4514-9e49-403279380b9b", "code": "BGBG.00.001", "title": {"en": "Diversity of organisms", "et": "Organismide mitmekesisus"}, "credits": 6, "state": {"code": "closed", "et": "Suletud", "en": "closed"}, "last_update": "2024-02-05T11:04:36", "alternatives": [{"uuid": "7b187ce6-a0ff-4511-c5a3-68da00a42d75", "code": "LOOM.01.121", "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36", "title": {"en": "Introduction to Eukaryotes - Protists and Fungi", "et": "Sissejuhatus eukarüootide mitmekesisusse - protistid ja seened"}, "credits": 2, "type": {"code": "regular", "et": "Tavaline aine", "en": "Regular course"}}, {"uuid": "20acc04c-c854-81f0-13f9-cda13df607b7", "code": "LOOM.00.003", "state": {"code": "closed", "et": "Suletud", "en": "closed"}, "last_update": "2024-02-05T11:04:36", "title": {"en": "Diversity of Organisms", "et": "Organismide mitmekesisus"}, "credits": 6, "type": {"code": "regular", "et": "Tavaline aine", "en": "Regular course"}}]}, {"required": false, "uuid": "080f6aaa-61d3-b2a5-5cda-d5d6c41000ea", "code": "LOOM.01.106", "title": {"en": "Taxonomy and Biodiversity Informatics", "et": "Biosüstemaatika ja elurikkuse informaatika"}, "credits": 1, "state": {"code": "closed", "et": "Suletud", "en": "closed"}, "last_update": "2024-02-05T11:04:36"}], "is_enlight_course": false}, "overview": {"study_languages": [{"code": "et", "et": "eesti keel", "en": "Estonian"}], "description": {"en": "The course introduces the major taxonomic groups of fungi, distinguished on the basis of recent advancements in molecular phylogeny. The importance of fungi is presented according to their general circulation of nutrients, focusing on different types of interactions (symbiotic, saprotrophic, parasitic) of fungi with members of other major organismal groups. The students will also learn about the many ways that humans directly encounter fungi via an introduction to phytopathology, medical mycology, biotechnology etc.", "et": "Kursusel tutvustatakse seeneriigi arengulugu, selle käsitlusi mükoloogias, suuremaid taksonoomilisi rühmi ja nende silmapaistvamaid esindajaid. Vaadeldakse seente tähtsust üldises aineringes, seente erinevaid toitumistüüpe ning sümbioosil, lagundamisel ja parasitismil põhinevaid suhteid teiste organismirühmade esindajatega. Kursusel tutvustatakse ka inimeste otseseid kokkupuuteid seentega läbi meditsiini-, ehitusmükoloogia, fütopatoloogia, biotehnoloogia jt. valdkondade."}, "objectives": [{"en": "The aim of this course is to present an overview of the phylogeny, classification, ecology, physiology and genetics of fungi with emphasis on interactions with other organisms.", "et": "Loengukursuse eesmärgiks on anda ülevaade seente fülogeneesist, klassifikatsiooniist, levikumustritest, ökoloogiast, füsioloogiast ja geneetikast. Lähemalt tutvustatakse ka seente interaktsioone teiste organismidega."}], "learning_outcomes": [{"en": "The student has gained knowledge in fungal taxonomy, ecology and genetics as well as in applied mycology. The student recognizes main taxonomic groups of fungi and their representatives, the role of fungi in interactions and similarities/specific characteris compared with other organisms. The student understands the main trends in mycology in the past and in the present, and the possibilities of application of fungi by humans.", "et": "Üliõpilane tunneb seente suuremaid taksonoomilisi rühmi ja nende olulisemaid esindajaid, seente rolli looduse  aineringes ja oskab kirjeldada nende suhteid, aga ka sarnasusi/erinevusei teiste organismirühmade esindajatega; mõistab mükoloogia seniseid ja praeguseid arengusuundi ning seente kasutusvõimalusi."}], "notes": {}}, "grading": {"independent_work": {"et": "Üliõpilane töötab läbi kursuse materjalid, läbib tunnid ja testid, mis kõik asuvad Moodle keskkonnas."}, "independent_work_assessments": [{"work_type": {"code": "report", "et": "Aruanne", "en": "report"}, "description": {"et": "Kursus loetakse sooritatuks kui tudeng on kogunud vähemalt 50% maksimaalsest punktisummast; hinne A: 90-100%, B: 80-90%, C: 70-80%, D: 60-70%, E: 50-60%"}}], "grade_preconditions": {"et": "Tudeng on töötanud läbi Moodle keskkonnas kättesaadavad loengumaterjalid, sooritanud seal olevad tunnid ja testid (ületades selleks etteantud lävendid) ning läbinud neli kohustuslikku praktikumi."}, "grade_evaluation": {"et": "Testide ja eksami sooritamise eest saadud punktide summa."}, "debt_elimination": {"et": "kokkuleppel õppejõuga"}, "assessment_scale": {"code": "grade", "et": "Eristav (A, B, C, D, E, F, mi)", "en": "differentiated (A, B, C, D, E, F, not present)"}, "results_available_from": {}}, "resources": {"web_based": {"code": "2", "et": "põimõpe", "en": "blended learning"}, "website_url": "http://moodle.ut.ee", "is_moodle_connected": false}, "participants": {"lecturers": [{"person_uuid": "22c39d3e-ca10-a052-cb2b-225c3f5045b2", "person_name": "Piret Lõhmus", "structural_unit": {"code": "LTOM01TA", "name": {"en": "Chair of Plant Ecology", "et": "taimeökoloogia õppetool"}, "address": "J. Liivi 2\n50409 Tartu linn, \nTartu linn, Tartumaa \nEST", "city": "Tartu linn", "street": "J. Liivi 2", "zip": "50409", "level": 4, "academic": false, "supports_continuous_learning": false, "parent_code": "LTOM01"}, "is_responsible": false, "is_teaching": true}, {"person_uuid": "c180d1ca-43fd-f10a-20b9-95b1ef971c8e", "person_name": "Ave Suija", "structural_unit": {"code": "LTOM01MK", "name": {"en": "Chair of Mycology", "et": "mükoloogia õppetool"}, "address": "J. Liivi 2\n50409 Tartu linn, \nTartu linn, Tartumaa \nEST", "city": "Tartu linn", "street": "J. Liivi 2", "zip": "50409", "level": 4, "academic": false, "supports_continuous_learning": false, "parent_code": "LTOM01"}, "is_responsible": false, "is_teaching": true, "study_group_roles": {"2. rühm": {"code": "2", "et": "Praktikumijuhendaja", "en": "supervisor of pract