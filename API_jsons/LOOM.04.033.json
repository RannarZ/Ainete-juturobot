quired": true, "uuid": "1b1f3773-40f3-672a-9e24-787b67bf03e4", "code": "BGZH.02.012", "title": {"en": "Fundamentals of Hydrobiology", "et": "Üldine hüdrobioloogia"}, "credits": 3, "state": {"code": "closed", "et": "Suletud", "en": "closed"}, "last_update": "2024-02-05T11:04:36", "alternatives": [{"uuid": "d78e8f63-6ef9-b41d-ded6-4ea5b4a70330", "code": "LOOM.04.034", "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36", "title": {"en": "Fundamentals of Hydrobiology", "et": "Üldine hüdrobioloogia"}, "credits": 3, "type": {"code": "regular", "et": "Tavaline aine", "en": "Regular course"}}]}], "is_enlight_course": false}, "overview": {"study_languages": [{"code": "et", "et": "eesti keel", "en": "Estonian"}], "other_languages": [{"code": "en", "et": "inglise keel", "en": "English"}], "description": {"en": "Water resources, biological aspects of their utilisation and assessment of supplies, implications of utilisation on the dynamics of water resources, the principles of water protection and management, the economical management and protection of catchment basins, restoration of water supplies, biomanipulation, poisonous metabolites of hydrobionts, the principles of water monitoring.", "et": "Kaasaegne rakendushüdrobioloogia kontseptsioon - see on süsteemsete ökoloogiliste teadmiste rakendamine veekogude ressursside kasutamisel, korraldamisel ja säilitamisel. Eluliselt tähtsaks eesmärgiks tänapäeval on veekogude ökosüsteemsete teenuste taastamine ja nende kestlikkuse tagamine. Käesolev kursus käsitleb veeökosüsteemide kasutamisega seotud aktuaalseid probleeme (nt eutrofeerumine, kalasaakide vähenemine, veekogude hüdrmorfoloogilised muutused) ja nende lahendusvõimalusi. Kursus koosneb kolmest teemade valdkonnast: 1) eutrofeerumine ja järvede taastamine, 2) jõgede toimimine ja taastamine, 3) kalakoosluste taastamine ja kaitse. "}, "objectives": [{"en": "<under construction>", "et": "Üliõpilased saavad ülevaate veekogude eutrofeerumisega ja veekogude ressursside kasutamisega seonduvatest aktuaalsetest probleemidest. Omandavad veekogude ressursside säästliku kasutamise ja haldamise ökosüsteemi-põhise käsituse."}], "learning_outcomes": [{"en": "After passing the course students", "et": "Õppeaine läbinud üliõpilased:"}, {"en": "<under construction>", "et": "Õppeaine edukal läbimisel üliõpilased:\nOrienteeruvad veekogude eutrofeerumise probleemistikus.\nOmavad ülevaadet veekogude taastamise meetoditest, oskavad hinnata nende rakendamise võimalusi ja mõju.\nOskavad selgitada säästliku kalanduse põhimõtteid.\nOskavad välja tuua inimtegevuse otsesed ja kaudsed mõjud veeorganismide populatsioonidele, veeökosüsteemide struktuurile ning mitmekesisusele.\nOn tuttavad veekogude troofilise ja ökoloogilise seisundi hindamisega.\nOn kursis Eesti veekeskkonna seisundiga ja selle seirega.\nOn võimelised iseseisvalt otsima ning analüüsima asjakohast informatsiooni."}], "notes": {"et": "puudub"}}, "grading": {"independent_work": {"et": "Ülesanded antakse loengute käigus, vt õppematerjalid."}, "independent_work_assessments": [{"work_type": {"code": "written", "et": "Kirjalik lõpphindamine", "en": "final written assessment"}, "description": {"et": "51% õigeid vastuseid kirjalikul eksamil tagab positiivse hinde"}}], "grade_preconditions": {"et": "Eritingimusi ei ole."}, "grade_evaluation": {"et": "Kirjalik eksam."}, "debt_elimination": {"et": "Võlgnevuste likvideerimine toimub kokkuleppel õppejõuga."}, "assessment_scale": {"code": "grade", "et": "Eristav (A, B, C, D, E, F, mi)", "en": "differentiated (A, B, C, D, E, F, not present)"}, "results_available_from": {}}, "resources": {"web_based": {"code": "2", "et": "põimõpe", "en": "blended learning"}, "website_url": "http://moodle.ut.ee", "is_moodle_connected": true, "learning_environments": [{"uuid": "4d0a9356-d3b1-4577-bc63-fe1380295660", "type": "MOODLE", "code": "LOOM.04.033", "base_url": "undefined", "is_auto_update": true, "is_default": true, "is_aut