vels": [{"code": "doctoral", "et": "doktoriõpe", "en": "doctoral studies"}], "hours": {"lecture": 0, "practice": 0, "seminar": 32, "colloquium": 0, "web_learning": 0, "independent_work": 46, "individual_work": 0, "internship": 0}, "prerequisites": [{"required": false, "uuid": "3b01d391-1fce-e527-cf23-1804392b0043", "code": "SOZU.00.012", "title": {"en": "Doctoral Seminar: Preparations for Publishing", "et": "Doktoriseminar: publitseerimise ettevalmistus"}, "credits": 3, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36"}], "is_enlight_course": false}, "overview": {"study_languages": [{"code": "et", "et": "eesti keel", "en": "Estonian"}, {"code": "en", "et": "inglise keel", "en": "English"}], "description": {"en": "The structure of scientific articles. Requirements to scientific articles. Argumentation: cohesion, logic, transparency. The style of scientific articles and its variation. Peer-reviewing.", "et": "Teadusliku artikli ülesehitus. Teadusartiklile esitatavad nõuded. Argumentatsioon: sidusus, loogilisus, selgus. Teadusartikli stiil ja selle variatsioonid. Retsenseerimine."}, "objectives": [{"en": "The objective of the course is to assist doctoral students in publishing their academic research in scientific journals of their field and in peer-reviewed collections of papers issued by international publishing houses.", "et": "Kursuse eesmärk on aidata kaasa doktorandi teadustöö publitseerimisele erialaajakirjades ja rahvusvaheliselt tunnustatud teaduskirjastuste juures ilmuvates ja/või eelretsenseeritavates artiklikogumikes."}], "learning_outcomes": [{"en": "After passing the course a doctoral student is able to write and review manuscripts of scientific articles.", "et": "Ainekursuse edukal läbimisel oskab doktorant kirjutada ja kriitiliselt hinnata (retsenseerida) teadusliku artikli käsikirja."}], "notes": {}}, "grading": {"independent_work": {"et": "1. Teadusliku artikli käsikirja koostamine ja selle esi