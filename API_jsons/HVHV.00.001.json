fo": {"is_vota_course": false, "is_continuous_learning_course": true, "study_levels": [{"code": "bachelor", "et": "bakalaureuseõpe", "en": "bachelor's studies"}, {"code": "master", "et": "magistriõpe", "en": "master's studies"}, {"code": "doctoral", "et": "doktoriõpe", "en": "doctoral studies"}], "hours": {"lecture": 2, "practice": 22, "seminar": 4, "independent_work": 50, "individual_work": 0, "internship": 0}, "is_enlight_course": false}, "overview": {"study_languages": [{"code": "et", "et": "eesti keel", "en": "Estonian"}], "description": {"en": "During the course, the students get acquainted with the possibilities of applying spatial data in the humanities using open-source applications and software (QGIS). The course will be taught mostly through practical assignments.", "et": "Kursusel tutvutakse ruumiandmete rakendusvõimalustega humanitaarteadustes, kasutades vabavaralisi rakendusi ja tarkvara (QGIS). Õpe toimub praktikumidena, aine läbimiseks tuleb lahendada jooksvalt koduseid ülesandeid ning esitada üks praktiline töö."}, "objectives": [{"en": "The goal of this course is to introduce the main applications of GIS in the humanities. Combining spatial data with data from the humanities enables to characterize places in a more diverse manner, while also creating a spatial dimension for humanities data analysis and allowing to pose new research questions.", "et": "Aine eesmärgiks on tutvustada põhilisi geoinfosüsteemide kasutusvõimalusi humanitaarteadustes ning sellega seotud tarkvara ning valmisrakendusi. Ühelt poolt võimaldab ruumiandmete ja humanitaarteadustes kasutatavate andmete ühendamine olemasolevaid kohti mitmekülgsemalt iseloomustada, teiselt poolt annab see humanitaaria andmete analüüsile ka selge ruumilise dimensiooni või võimaldab esitada hoopis uusi uurimisküsimusi."}], "learning_outcomes": [{"en": "After completing this course, the student:", "et": "Aine läbinu:"}, {"en": "    knows, what GIS is;", "et": "*\tteab, mis on geoinfosüsteemid;"}, {"en": "    is familiar with the more common applications of spatial data in the digital humanities;", "et": "*\ttunneb digihumanitaarias enamlevinud ruumiandmete rakendusvõimalusi;"}, {"en": "    has the basic skills for visualizing and analyzing spatial data, using the necessary software and ready-made applications;", "et": "*\ttunneb algtasemel ruumiandmete visualiseerimise ja analüüsi tarkvara ja oskab kasutada erinevaid valmisrakendusi;"}, {"en": "    knows the principles of georeferencing;", "et": "*\ttunneb georefereerimise põhimõtteid;"}, {"en": "    is able to combine spatial data with other types of data layers (pictures, texts, frequency information etc.).", "et": "*\toskab kombineerida ruumiandmeid muude digitaalsete andmekihtidega (pildid, tekstid, sagedusinfo jne)."}], "notes": {}}, "grading": {"independent_work": {}, "independent_work_assessments": [{"work_type": {"code": "homework", "et": "Kodutöö", "en": "home work"}, "description": {"et": "Tudeng esitab kursuse jooksul kaks kodutööd, mis aitavad praktikumides õpitut kinnistada."}}, {"work_type": {"code": "practical", "et": "Praktiline ülesanne", "en": "practical assignment"}, "description": {"et": "Tudeng analüüsib kursusel õpitud meetodi(te) abil mõnd vabalt valitud või etteantud andmestikku, esitab selle kohta kirjaliku raporti ning esitleb oma tööd ka suuliselt (u 5 minutit)."}}], "grade_preconditions": {"et": "Kahe kodutöö ning praktilise töö õigetähtaegne esitamine, osavõtt vähemalt 70% auditoorsest õppetööst."}, "grade_evaluation": {"et": "Arvestuse saamiseks on vajalik täita ülalnimetatud kriteeriumid."}, "debt_elimination": {"et": "Kokkuleppel õppejõuga."}, "assessment_scale": {"code": "pass", "et": "Eristamata (arv, m.arv, mi)", "en": "non-differentiated (pass, fail, not present)"}, "results_available_from": {}}, "resources": {"web_based": {"code": "2", "et": "põimõpe", "en": "blended learning"}, "website_url": "http://moodle.ut.ee", "is_moodle_connected": true, "learning_environments": [{"uuid