ourse": false}, "overview": {"study_languages": [{"code": "et", "et": "eesti keel", "en": "Estonian"}], "description": {"en": "An overview of modern molecular immunology.", "et": "Ülevaade kaasaegsest molekulaarsest immunoloogiast."}, "objectives": [{"en": "The aim of the course is acquiring knowledge in the basics of immunology, developing understanding of the basic principles of contemporary molecular immunology and immunogenetics, gaining the ability to use this knowledge in practice and understanding the development and evolution of the immune system.", "et": "Kaasaegse immunoloogia põhialustega tutvumine ja nende omandamine. Arusaamine kaasaegse molekulaarse immunoloogia ja immunogeneetika põhiprintsiipidest, nende tööpõhimõtetest ja omandada oskus saadud teadmiste rakendamiseks praktilises (uurimis)töös. Immuunsüsteemi tekke ja evolutsiooni mõistmine."}], "learning_outcomes": [{"en": "Student has acquired the basics of immunology, understands them, and is able to describe the response of the innate and adaptive immune system to various antigens. The student knows the organs, cells and molecules of the immune system and special features of the immunogenetics. The student knows some diseases caused by deviations in the functioning of the immune system. Knows the main methods applied in laboratory work both for studying immunology and related those applying immunological principles. Knows the main model organisms used for studying immunology. The student understands the development and relationship of different parts of immune system from evolutionary perspective.", "et": "Üliõpilane on omandanud immunoloogia põhialused, saab neist aru, oskab kirjeldada organismi immuunsüsteemi vastuseid kaasasündinud ja omandatud immuunsüsteemide puhul. Teab Immuunsüsteemis osalevaid organeid, rakke ja molekule. Tunneb immunogeneetika iseärasusi. Oskab välja tuua mõned immuunsüsteemi kõrvalekalletest tingitud haiguslikud seisundid.  Tunneb peamisi laboritöös kasutatavaid immuunoloogiga seotud meetodite tööpõhimõtteid ning immunoloogia uurimises kasutatavaid peamisi meetodeid ning mudelorganisme. Mõistab immuunsüsteemi erinevate osade teket ja omavahelisi seoseid evolutsioonilisest lähtepunkitst."}], "notes": {}}, "grading": {"independent_work": {}, "independent_work_assessments": [{"work_type": {"code": "written", "et": "Kirjalik lõpphindamine", "en": "final written assessment"}, "description": {"et": "Eksam. 8 küsimust(1-4 punki).maksimaalselt 20 punkti.20-18p-A,18-16p-B,16-14p-C,14-12p-D, 12-10p-D. alla 10p-F"}}], "grade_preconditions": {"et": "Tudeng peab eksamile regitreeruma. Eksamile registreerimiseks peab tudeng olemae ainele registreeritud."}, "grade_evaluation": {"et": "Eksam. 8 küsimust(1-4 punki). Maksimaalselt 20 punkti.\n20-18p-A,\n18-16p-B,\n16-14p-C,\n14-12p-D, \n12-10p-D, \nalla 10p-F"}, "debt_elimination": {"et": "Kokkuleppel õppejõuga"}, "assessment_scale": {"code": "grade", "et": "Eristav (A, B, C, D, E, F, mi)", "en": "differentiated (A, B, C, D, E, F, not present)"}, "results_available_from": {}}, "resources": {"web_based": {"code": "2", "et": "põimõpe", "en": "blended learning"}, "website_url": "http://moodle.ut.ee", "is_moodle_connected": true, "learning_environments": [{"uuid": "ab025b5e-5718-4127-b1aa-4963208ecf6d", "type": "MOODLE", "code": "LOMR.02.001", "base_url": "undefined", "is_auto_update": false, "is_default": true, "is_auto_grading": false}]}, "participants": {"lecturers": [{"person_uuid": "75ecf5da-bb86-3c72-285f-e61ffbbef077", "person_name": "Arnold Kristjuhan", "structural_unit": {"code": "LTMR06", "name": {"en": "Chair of Cell Biology", "et": "rakubioloogia õppetool"}, "phone": "+372 737 5022", "address": "Riia 23b, ruumid 123-127, 135, 220, 227\n51010 Tartu linn, \nTartu linn, Tartumaa \nEST", "city": "Tartu linn", "street": "Riia 23b, ruumid 123-127, 135, 220, 227", "zip": "51010", "level": 3, "academic": true, "supports_continuous_learning": false, "parent_code": "LTMR"}, "is_responsible": true, "i