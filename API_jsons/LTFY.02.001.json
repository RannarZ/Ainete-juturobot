: 92, "individual_work": 0, "internship": 0}, "prerequisites": [{"required": true, "uuid": "57be53cb-0fe5-b2be-3360-ed7b5743782c", "code": "LTFY.02.003", "title": {"en": "Review Course of Materials Science", "et": "Materjaliteaduse ülevaatekursus"}, "credits": 6, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36"}, {"required": true, "uuid": "d70ab9dd-28f4-3241-0e68-812c10c83b39", "code": "LTFY.02.002", "title": {"en": "Classes and Technologies of Materials", "et": "Materjaliklassid ja nende tehnoloogiad"}, "credits": 12, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36"}], "is_enlight_course": false}, "overview": {"study_languages": [{"code": "et", "et": "eesti keel", "en": "Estonian"}], "description": {"en": "Description of the structure of matter on several levels is studied in this course. Emphasis is payed on relations of the applications of the materials with their properties and structure. Practical problems bounded with lecture topics are solved in seminars.", "et": "Kursuses käsitletakse aine ehitust selle erinevatel struktuuritasanditel ning materjalide rakenduskohtade seost nende omaduste ja ehitusega. Seminarides lahendatakse kursuse sisuga seonduvaid ülesandeid."}, "objectives": [{"en": "The goal of the course is deepening of the knowledge of students about structure of materials making them acquainted with the basic concepts of solid state physics. Relations between the properties of materials and their structure are explained.", "et": "Kursuse eesmärk on süvendada üliõpilaste ettekujutust aine ehitusest ja struktuurist, tutvustades tahkisefüüsika põhimõisteid ja kontseptsioone ning selgitada materjalide omaduste tulenemist materjalide koostisest ja ehitusest."}], "learning_outcomes": [{"en": "After the course a student:", "et": "Kursuse läbinud üliõpilane:"}, {"en": "1. Understands the basic ideas and models of quantum mechanics and crystallography, necessary to understand the processes in materials on the atomic level.", "et": "1. Tunneb kvantmehhaanika ja kristallograafia põhiseisukohti ja -mudeleid, mis on vajalikud aines atomaarsel tasandil toimuvate protsesside mõistmiseks."}, {"en": "2. Comprehends the structure and properties of solid materials of different level of ordering.", "et": "2. Omab ettekujutust erineva korrastusastmega tahkiste struktuurist ja omadustest."}, {"en": "3. Comprehends the dynamics of crystals and the processes therein, their relation to the properties of materials.", "et": "3. Omab ettekujutust kristallide dünaamikast, neis toimuvatest protsessidest ja seostest materjalide omadustega."}, {"en": "4. Understands, how the phase composition and spatial structuring influences properties of materials.", "et": "4. Teab, kuidas materjali faasiline koostis ja ruumiline struktureeritus mõjutab selle omadusi."}, {"en": "5. Is able to solve the course-related problems.", "et": "5. Oskab lahendada ülesandeid kursuses käsitletud teemade kohta."}], "notes": {"et": "(toimub 6. sem)"}}, "grading": {"independent_work": {}, "independent_work_assessments": [{"work_type": {"code": "test", "et": "Kontrolltöö", "en": "test"}, "description": {"et": "4 kontrolltöö positiivne sooritus"}}], "grade_preconditions": {"et": "Kõigi kontrolltööde sooritamine."}, "grade_evaluation": {"et": "Kontrolltööde summaarse tulemuse alusel. Kokkuleppel on võimalik hinde muutus ühe astme võrra individuaalse järelkontrolli põhjal."}, "debt_elimination": {"et": "Iga kontrolltöö jaoks on võimalik semestri jooksul üks kordussooritus.\nEksami kordussooritus on võimalik üks kord enne semestri lõppu.\nKorduseksami peab sooritama hiljemalt järgneva semestri alguseks.\nKorduseksami tähtaega võib dekaan õppejõu taotlusel pikendada kuni 2 nädala võrra."}, "assessment_scale": {"code": "grade", "et": "Eristav (A, B, C, D, E, F, mi)", "en": "differentiated (A, B, C, D, E, F, not present)"}, "results_available_from": {}}, "reso