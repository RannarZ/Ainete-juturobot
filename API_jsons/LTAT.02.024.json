es": {}}, "grading": {"independent_work": {}, "independent_work_assessments": [{"work_type": {"code": "homework", "et": "Kodutöö", "en": "home work"}, "description": {"et": "Iga loengu järel on tarvis kas läbida lühike test või kirjutada lühike kokkuvõte või teha mingi ülesanne. Kodutöö peab olema edukalt sooritatud vähemalt 70% loengute kohta. \n\n"}}, {"work_type": {"code": "project", "et": "Projekt", "en": "project"}, "description": {"et": "Kursuse jooksul on vaja läbi viia ja esitada projekt, mis haakub loengus käsitletud teemadega. Projekti juhendajateks on õppejõud või teised  töörühma liikmed."}}], "grade_preconditions": {}, "grade_evaluation": {"et": "Projekt peab olema sooritatud, 70% loengute kohta peab olema edukalt sooritatud kodutöö."}, "debt_elimination": {}, "assessment_scale": {"code": "pass", "et": "Eristamata (arv, m.arv, mi)", "en": "non-differentiated (pass, fail, not present)"}, "results_available_from": {}}, "resources": {"web_based": {"code": "2", "et": "põimõpe", "en": "blended learning"}, "website_url": "http://moodle.ut.ee", "is_moodle_connected": false}, "participants": {"lecturers": [{"person_uuid": "0c8487fd-7d26-55b1-2669-7808a470a238", "person_name": "Jaan Aru", "structural_unit": {"code": "LTAT02", "name": {"en": "Chair of Data Science", "et": "andmeteaduse õppetool"}, "level": 3, "academic": true, "supports_continuous_learning": false, "parent_code": "LTAT"}, "is_responsible": true, "is_teaching": true}, {"person_uuid": "0038ea3c-91e9-813b-dc3f-1871fefb5c49", "person_name": "Kadi Tulver", "structural_unit": {"code": "LTAT02", "name": {"en": "Chair of Data Science", "et": "andmeteaduse õppetool"}, "level": 3, "academic": true, "supports_continuous_learning": false, "parent_code": "LTAT"}, "is_responsible": false, "is_teaching": true}, {"person_uuid": "6e061cfd-9bde-f3dc-5160-0be3f75a8259", "person_name": "Taavi Kivisik", "structural_unit": {"code": "LTAT02", "name": {"en": "Chair of Data Science", "et": "andmeteaduse õppetool"}, "leve