dent_work": 78, "individual_work": 0, "internship": 0}, "prerequisites": [{"required": true, "uuid": "a0e8ddea-04ba-3188-46f5-5cad4af2e2c5", "code": "ARFS.01.063", "title": {"en": "Human Physiology", "et": "Inimese füsioloogia"}, "credits": 9, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36"}, {"required": true, "uuid": "93e97bd9-9e66-5731-9f5a-a0e0eda3123c", "code": "ARMB.00.001", "title": {"en": "Medical Microbiology", "et": "Meditsiiniline mikrobioloogia"}, "credits": 6, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36"}, {"required": true, "uuid": "7ff6b7f8-879c-d5b1-dd94-472f959413b1", "code": "ARBK.01.043", "title": {"en": "Medical Biochemistry", "et": "Meditsiiniline biokeemia"}, "credits": 5, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36"}, {"required": true, "uuid": "aba409fe-e4eb-7588-d76a-9dd961b8ba8b", "code": "ARBK.01.042", "title": {"en": "Organic and Bioorganic Chemistry", "et": "Orgaaniline ja bioorgaaniline keemia"}, "credits": 5, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36"}], "is_enlight_course": false}, "overview": {"study_languages": [{"code": "et", "et": "eesti keel", "en": "Estonian"}], "description": {"en": "The course of pathophysiology for pharmacy students consists of etiology and pathogenesis of main syndromes and diseases in the hierarchic subsystems (organ systems, organs, tissues, cells, subcellular elements). Alteration in the function of any subsystem affects other subsystems as well as the system as whole. In cases in which the mechanism of disease and the mechanism of clinical intervention are well understood, the treatment is said to be rational, or clearly based on scientific knowledge.", "et": "Kursuses käsitletakse põhiliste sündroomide ja haiguste etioloogiat ja patogeneesi organi, koe ja raku tasemel ning haiguslike muutuste toimet organismi kui terviku funktsioonidele. Patoloogilise füsioloogia kursuse läbimine aitab mõista patoloogiliste protsesside ja haiguste arengu seaduspärasusi ning nende protsesside medikamentoosse mõjustamise võimalusi."}, "objectives": [{"en": "The aim of the course is to provide students with basic knowledge human body normal operation disturbances - typical pathological proceses and pathogenesis mechanisms of  diseases as well treatment principles.", "et": "Õppekava eesmärgiks on anda üliõpilastele baasteadmisi organismi normaalse talitluse häiretest - tüüpilistest patoloogilistest protsessidest ja sagedasemate haiguste patogeneesimehhanismidest ning ka raviprintsiipidest."}], "learning_outcomes": [{"en": "Learning outcomes\n\nStudents who have completed  the  course:\n- is able to explain the changes in the functions of human organ systems in pathological processes and the  most common diseases at both cell and organism level.\n- knows and is able to associate the development mechanisms of disorders caused by harmful factors and also treatment principles.\n- know and analyze the  basic regulatory mechanisms  that ensure the stability of  the internal environment.", "et": "Kursuse läbinud üliõpilane: \n- oskab selgitada inimese elundsüsteemide funktsioonide muutusi patoloogiliste protsesside ja enamlevinud haiguste puhul nii raku kui ka organismi tasemel \n-  teab  ja oskab seostada kahjustavate faktorite poolt põhjustatud häirete arengumehhanisme ja ka raviprintsiipe.\n- nimetab ja analüüsib põhilisi regulatoorseid mehhanisme, mis tagavad sisekeskkonna püsivuse."}], "notes": {}}, "grading": {"independent_work": {"et": "Rühmatöö - rühmatöö tulemusena valmib semestri lõpuks infomaterjal, mis selgitab patsiendile arusaadavalt konkreetset patoloogilist protsessi/mehhanismi.  Täpsem juhis rühmatöö tegemiseks ja hinde kujunemisest antakse esimeses seminaris. Juhis on ka Moodle's"}, "independent_work_assessments": [{"work_type": {"code": "test", "et": "Kontrolltöö", "en": "test"}, "description": {"et": "Semestri jooksul on 3 hindelist kontrolltööd, mis toimuvad iga 3-4 nädala tagant ja on vastavalt 3-4 teema peale.\n\nKontrolltöös esitatakse iga käsitletava teema kohta 2 lühiküsimust mõistete selgitamiseks ning 1 pikemat selgitus  vajava  küsimuse (protsessi kirjeldus või nt patsiendijuhtum)."}}], "grade_preconditions": {"et": "2023/2024 õppeaasta kevadsemestril kujuneb aine lõpptulemus jooksva töö alusel. Selle eelduseks on:\n- puududa võib kahest seminarist\n- rühmatöö peab olema esitatud koos kaasõpilase hinnanguga\n- kontrolltööd on positiivselt sooritatud\n- lubatud puudumiste arvu ületamisel on kursus mittearvestatud"}, "grade_evaluation": {"et": "Hinne kujuneb rühmatöö ja kontrolltööde keskmisena. Täpsem hindamise põhimõte on kirjas Moodle's ja arutatakse läbi ka 1. seminaris."}, "debt_elimination": {"et": "1) Kontrolltöid saab järele vastata järgmise kontrolltöö seminaris ning semestri kahel viimasel nädalal\n2) seminari võlgnevuste likvideerimiseks võtta ühendust mall.eltermaa@ut.ee"}, "assessment_scale": {"code": "grade", "et": "Eristav (A, B, C, D, E, F, mi)", "en": "differentiated (A, B, C, D, E, F, not present)"}, "results_available_from": {}}, "resources": {"web_based": {"code": "2", "et": "põimõpe", "en": "blended learning"}, "website_url": "http://moodle.ut.ee", "is_moodle_connected": true, "learning_environments": [{"uuid": "cb43432e-a535-47ed-9503-f69d5d81f998", "type": "MOODLE", "code": "ARMP.03.031", "base_url": "undefined", "linked_to_groups": ["423f0323-877b-01e7-ca2f-95c3754f3ed2", "19b823dc-1c4f-58a8-457c-a2c18994f5a6"], "is_auto_update": false, "is_default": true, "is_auto_grading": false}], "mandatory_materials": "1. Color Atlas of Pathophysiology. Stefan Silbernagl, Florian Lang. Thieme,Stuttgart, 2000,2010, 2016", "recommended_materials": "2. Kathryn L. McCance, Sue E. Huether Pathophysiology. The Biologic Basis for Disease in Adults and Children 5th edition Mo