": 0, "independent_work": 70, "individual_work": 0, "internship": 0}, "prerequisites": [{"required": true, "uuid": "e979d518-d3bb-d364-882f-b96ebf9d7c40", "code": "MV00.00.015", "title": {"en": "Internal Medicine", "et": "Sisehaigused"}, "credits": 28, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-04-01T15:19:05", "alternatives": [{"uuid": "5c387d45-b118-4ab8-c88a-b4f5cbf65b8b", "code": "AR00.00.065", "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36", "title": {"en": "Internal Medicine", "et": "Sisehaigused"}, "credits": 27, "type": {"code": "complex", "et": "Kompleksaine", "en": "Complex course"}}]}, {"required": true, "uuid": "21eb468a-9a12-43e0-94c7-78a7cf4ff623", "code": "AR00.00.066", "title": {"en": "Surgery", "et": "Kirurgia"}, "credits": 12, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-04-01T15:21:36"}], "is_enlight_course": false}, "overview": {"study_languages": [{"code": "et", "et": "eesti keel", "en": "Estonian"}], "description": {"en": "Introduces students to the main principles of organization and financing of health care systems. The regulation, financing and provision of public health and health care services in Estonia will be studied. Performance of health care systems and services will be analyzed in the global context.", "et": "Aine annab ülevaate tervishoiu juhtimise ja rahastamise põhimõtetest ning tervishoiu korraldusest Eestis võrdluses teiste Euroopa maadega. Üliõpilased saavad ülevaate tervishoiuteenuste hindamisest, tervishoiuökonoomika alustest, meetoditest ja rakendustest tervishoiu aktuaalsete probleemide lahendamisel."}, "objectives": [{"en": "Develop basic knowledge on principles of organization, regulation, and financing of health care and public health systems. Develop a basic understanding of the assessment of health care services. To understand the dilemmas inherent to the provision of health services.", "et": "- omandada ülevaade arstiabi ja rahvatervishoiu organisatsioonist ja rahastamisest Eestis;\n- analüüsida arstiabis ja tervishoius esinevaid kitsaskohti ja pakkuda neile lahendusi.\n- mõista tervishoiuteenuste hindamist ja tervishoiuökonoomika aluseid ning rakendamisvõimalusi tõenduspõhisel otsustamisel arstiabis ja tervishoiu juhtimisel;"}], "learning_outcomes": [{"en": "The student will be expected:", "et": "Õppeaine läbinu:"}, {"en": "- to understand and explain the organization of public health and health care systems in different countries;", "et": "- tunneb Eesti tervishoiu ja rahvatervise korraldust;"}, {"en": "- to explain the aim and implementation of reforms of health services and health care systems and to analyze these in the global context;", "et": "- oskab ära tunda ja mõistab arstiabiga kaasnevaid ja tervishoiusüsteemile iseloomulikke dilemmasid ja neid arusaadavalt selgitada;"}, {"en": "- to understand the basics of health and economic impact assessment in clinical medicine and public health.", "et": "- oskab leida ja kasutada tõenduspõhiseid andmeid otsuste tegemiseks tervishoius."}], "notes": {}}, "grading": {"independent_work": {"et": "Iseseisvate tööde temaatika:\n1) teadusartikli kriitiline analüüs (suuline) tõenduspõhisest otsustamisest arstiabis\n2) ettekanne - kvaliteet ja kättesaadavus, reeglid ja õiglus Eesti tervishoius"}, "independent_work_assessments": [{"work_type": {"code": "oral", "et": "Suuline lõpphindamine", "en": "final oral assessment"}, "description": {}}], "grade_preconditions": {"et": "Osalemine vähemalt 50% loengutest ja 80% seminaridest.\nIseseisvate tööde õigeaegne esitamine."}, "grade_evaluation": {"et": "Arvestusel tuleb vastata kahele küsimusele, mis on esitatud kordamisküsimustes.\nErandjuhul on võimalik lõpptulemuse kujunemine ilma suulist arvestust sooritamata, kui osalus loengutes ja seminarides on vähemalt 85% ja iseseisvad tööd on esitatud tähtaegselt väga heale tulemusele."}, "debt_eli