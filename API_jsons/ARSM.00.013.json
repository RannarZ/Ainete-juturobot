"study_levels": [{"code": "bachelor_master", "et": "integreeritud bakalaureuse- ja magistriõpe", "en": "integrated bachelor's and master's studies"}], "hours": {"lecture": 14, "practice": 20, "seminar": 5, "colloquium": 0, "web_learning": 0, "independent_work": 13, "individual_work": 0, "internship": 0}, "prerequisites": [{"required": true, "uuid": "e979d518-d3bb-d364-882f-b96ebf9d7c40", "code": "MV00.00.015", "title": {"en": "Internal Medicine", "et": "Sisehaigused"}, "credits": 28, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-04-01T15:19:05", "alternatives": [{"uuid": "5c387d45-b118-4ab8-c88a-b4f5cbf65b8b", "code": "AR00.00.065", "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-02-05T11:04:36", "title": {"en": "Internal Medicine", "et": "Sisehaigused"}, "credits": 27, "type": {"code": "complex", "et": "Kompleksaine", "en": "Complex course"}}]}, {"required": true, "uuid": "21eb468a-9a12-43e0-94c7-78a7cf4ff623", "code": "AR00.00.066", "title": {"en": "Surgery", "et": "Kirurgia"}, "credits": 12, "state": {"code": "confirmed", "et": "Kinnitatud", "en": "confirmed"}, "last_update": "2024-04-01T15:21:36"}], "is_enlight_course": false}, "overview": {"study_languages": [{"code": "et", "et": "eesti keel", "en": "Estonian"}], "other_languages": [{"code": "en", "et": "inglise keel", "en": "English"}], "description": {"en": "Lectures and practical classes address the principles and possibilities of rehabilitation in various disorders: disorders and diseases of the support and movement system, brain damage, oncology, cardiovascular diseases. They learn to objectively evaluate various functional impairments, learn about the work of different team profiles. Case studies and various evidence-based treatment options are addressed.", "et": "Loengutes ja praktikumides käsitletakse  taastusravi põhimõtteid ja võimalusi erinevate funktsioonihäirete puhul:  tugi- ja liikumissüsteemi häired ja haigused, ajukahjustus, onkoloogiline haigus, kardivaskulaarsüsteemi haigused. Õpitakse objektiivselt hindama erinevaid funktsioonihäireid, tutvutakse erinevate meeskonnaliimete tööga. Käsitletakse haigusjuhte ja erinevaid tõenduspõhiseid ravivõimalusi."}, "objectives": [{"en": "To provide knowledge about principles and possibilities of rehabilitation in case of various functional disorders, to introduce The work of various members of the team (rehab, physiotherapist, occupational therapist, psychologist, speech therapist) and various methods of treatment.", "et": "Anda teadmised  taastusravi põhimõtetest ja  võimalustest erinevate funkstioonihäirete puhul, Tutvustada \n erinevate meeskonnaliikmete ( taastusarst. füsioterapeut, tegevusterapeut, psühholoog, logopeed) tööd ja erinevaid ravimeetodeid."}], "learning_outcomes": [{"en": "It is oriented towards rehabilitation as a team specialist. Knows the principles of assessing patient functional reserves, and is able to evaluate the most common functional disorders and to prepare a rehabilitation plan.Approaches to rehabilitation in the case of various functional disorders", "et": "Orienteerub  taastusravi kui meeskondliku eriala põhimõtetes. Teab patsiendi funktsionaalsete reservide hindamise põhimõtteid ja oskab hinnata sagedasemaid funktsioonihäireid ning koostada taastusravi plaani. \nOrienteerub taastusravi võimalustes erinevate funktsioonihäirete puhul"}], "notes": {}}, "grading": {"independent_work": {"et": "Loend kõikidest iseseisvalt tehtavatest ülesannetest Moodles: kroonilise valu loengu test, H5P onkoloogilise haige taastusravi põhimõtted, kodune töö 1, test 100% ( test muutub avatavaks neile, kes on täitnud anonüümse tagasiside küsimustiku)."}, "independent_work_assessments": [{"work_type": {"code": "practical", "et": "Praktiline ülesanne", "en": "practical assignment"}, "description": {"et": "Üliõpilane demonstreerib reaalse patsiendi peal anamneesi võtmist ja vastava probleemi kliinilist uurimist, seab taastusravi eesmärgid ning koostab taastusraviplaani arvestades patsiendi funktsionaalset seisundit ja kaasuvaid haigusi.  "}}, {"work_type": {"code": "case_analysis", "et": "Juhtumi analüüs (kaasus)", "en": "case analysis"}, "description": {"et": "Taastusravi plaan on koostatud arvestades patsiendi funktsionaalset seisundit ja kaasuvaid haigusi; patsienti on käsitletud terviklikult."}}], "grade_preconditions": {"et": "Kõikide praktikumide läbimine ja iseisvate ülesannete esitamine Moodles."}, "grade_evaluation": {"et": "Eelduseks osavõtt kõikidest praktikumidest ja kõigi iseseisvate ülesannete esitamine Moodles. Lõpphinne kujuneb eelduste täitmisel ja seminaris arvestuse sooritamisel kas praktilise ülesandena reaalse patsiendi peal või juhtumianalüüsi suulisel lahendamisel."}, "debt_elimination": {"et": "Puudutud praktikumi teema läbimine, etteantud ülesande (sõltub teemast) esitamine ning arvestuse sooritamine."}, "assessment_scale": {"code": "pass", "et": "Eristamata (arv, m.arv, mi)", "en": "non-differentiated (pass, fail, not present)"}, "results_available_from": {}}, "resources": {"web_based": {"code": "2", "et": "põimõpe", "en": "blended learning"}, "website_url": "http://moodle.ut.ee", "is_moodle_connected": true, "learning_environments": [{"uuid": "4be549af-e596-4e59-bc0e-52f7615c619b", "type": "MOODLE", "code": "ARSM.00.013", "base_url": "undefined", "linked_to_groups": ["50feff6e-7c57-fd06-b98e-d9291ba5300f", "f5d44752-89a4-c3d9-7f92-199faef545e6", "4d94b495-e325-4a19-19a8-35bb05d8ed2b", "e78c8827-4193-5e07-4839-2452970b2b4f", "ecf13cf1-6f90-05d7-564e-acf2a6087dfa", "dc15d7cb-2681-0af4-5385-4f9310f6f77e"], "is_auto_update": false, "is_default": true, "is_auto_grading": false}], "mandatory_materials": "Moodle keskkonnas leitavad loengumaterjalid, praktikumide kaustades olevad materjalid"}, "participants": {"lecturers": [{"person_uuid": "8e338eb3-b1e2-e4de-701b-611541188dc8", "person_name": "Aet Lukmann", "structural_unit": {"code": "MVCM14", "name": {"en": "Dep